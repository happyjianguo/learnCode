<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.jansh.dao.mapper.WxdTemplateConfigMapper">
  	  <select id="selectByTempNo" parameterType="String"  resultType="WxdTemplateConfig" >
				SELECT
					 TEMPLATENO,TEMPLATETITLE,TEMPLATEID,TEMPLATEURL,TEMPLATECOLOR,TEMPLATEHFIRST,TEMPLATEFREMARK,PLATFORMID
				FROM 
					WXDTEMPLATECONFIG 
				WHERE 
					TEMPLATENO=#{tempNo}
		</select>
		
</mapper>