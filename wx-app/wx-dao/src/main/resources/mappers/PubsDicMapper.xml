<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.jansh.dao.mapper.PubsDicMapper">
  	  <select id="getUrl" parameterType="PubsDic"  resultType="PubsDic" >
				SELECT 
					NODETYPEDES, NODEVALUE
				FROM 
					PUBSDIC 
				WHERE 
					NODETAG= #{NODETAG} 
				AND 
					NODETYPE= #{NODETYPE}
		</select>
		
</mapper>