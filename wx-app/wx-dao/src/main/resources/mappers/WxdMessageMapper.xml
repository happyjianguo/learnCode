<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.jansh.dao.mapper.WxdMessageMapper">
  	  <select id="getWxdMessageById" parameterType="String"  resultType="WxdMessage" >
				SELECT 
					MSGTYPE,MEDIAID, CONTENT,TRANS
				FROM 
					WXDMESSAGE 
				WHERE 
					MSGID=#{msgId} 
		</select>
</mapper>