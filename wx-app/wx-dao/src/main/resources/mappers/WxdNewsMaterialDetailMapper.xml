<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.jansh.dao.mapper.WxdNewsMaterialDetailMapper">
	 <resultMap type="WxdNewsMaterialDetail" id="materialDetailId">
    	<result column="MATERIALTITLE" property="materialTitle" />
    	<result column="THUMBMEDIAURL" property="thumbMediaUrl" />
    	<result column="DIGEST" property="diGest" />
    	<result column="CONTENTSOURCEURL" property="contentSourceUrl" />
    </resultMap>
  	  <select id="getWNMDByMId" parameterType="String"  resultMap="materialDetailId" >
				SELECT 
					MATERIALTITLE,DIGEST,THUMBMEDIAURL,CONTENTSOURCEURL										 
				FROM 
					WXDNEWSMATERIALDETAIL 
				WHERE 
					MATERIALID= #{mid} 
		</select>
</mapper>