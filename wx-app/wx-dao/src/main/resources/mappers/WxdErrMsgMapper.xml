<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.jansh.dao.mapper.WxdErrMsgMapper">
	<select id="selectByErrorCode" parameterType="String"  resultType="WxdErrMsg" >
				SELECT
					 ERRORCODE,ERRORDES
				 FROM 
				 	WXDERRMSG 
				 WHERE 
				 	ERRORCODE=#{errorCode}
	</select>
</mapper>