     	<foreach collection="list" item="item" index="index" separator=";" >
			    UPDATE
			        CLOUDVIRGOOD
			     <set> 
			        <if test="item.prizename != null">    
			            PRIZENAME = #{item.prizename},    
			        </if>
			        <if test="item.prizestyle != null">    
			            PRIZESTYLE = #{item.prizestyle},    
			        </if> 
			        <if test="item.carrier != null">    
			            CARRIER = #{item.carrier},    
			        </if>
			        <if test="item.combovalue != null">    
			            COMBOVALUE = #{item.combovalue},    
			        </if> 
			        <if test="item.prizeprice != null">    
			            PRIZEPRICE = #{item.prizeprice},    
			        </if> 
			        <if test="item.pervalue != null">    
			            PERVALUE = #{item.pervalue},    
			        </if>
			        <if test="item.imagepath != null">    
			            IMAGEPATH = #{item.imagepath},    
			        </if>
			        <if test="item.status != null">    
			            STATUS = #{item.status},    
			        </if> 
			        <if test="item.note != null">    
			            NOTE = #{item.note},    
			        </if> 
